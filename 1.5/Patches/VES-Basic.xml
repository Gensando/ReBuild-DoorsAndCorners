<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Furniture Expanded - Art</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="VFE_Wallpaper"]/graphicData/texPath</xpath>
					<value>
						<texPath>Things/Buildings/Linked/VES_Wallpaper/Wall_Atlas_Wallpaper</texPath>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="VFE_Wallpaper"]/uiIconPath</xpath>
					<value>
						    <uiIconPath>Things/Buildings/Linked/VES_Wallpaper/Wallpaper_MenuIcon</uiIconPath>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Furniture Expanded - Architect</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="VFEArch_ArmoredWall"]/graphicData/texPath</xpath>
					<value>
						<texPath>Things/Buildings/Linked/VES_Wallpaper/Wall_Atlas_Wallpaper</texPath>
					</value>
				</li>
				<li Class="PatchOperationReplace">
					<xpath>/Defs/ThingDef[defName="VFEArch_ArmoredWall"]/uiIconPath</xpath>
					<value>
						    <uiIconPath>Things/Buildings/Linked/VES_Wallpaper/Wallpaper_MenuIcon</uiIconPath>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>